{
  "name": "@react-md/test-utils",
  "version": "1.0.0-alpha.0",
  "description": "Test utilities for react-md",
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "sideEffects": false,
  "scripts": {
    "dev": "npm-run-all --parallel build-esm-watch build-types-watch",
    "build": "npm-run-all build-esm build-types",
    "build-esm": "swc src -d ./",
    "build-esm-watch": "pnpm run build-esm --watch",
    "build-types": "tsc -P tsconfig.types.json",
    "build-types-watch": "pnpm run build-types --watch",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "clean": "rm -rf .turbo node_modules *.js *.d.ts",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlaursen/react-md.git",
    "directory": "packages/test-utils"
  },
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "keywords": [
    "react-md",
    "material design",
    "react",
    "test",
    "testing"
  ],
  "license": "MIT",
  "dependencies": {
    "@react-md/core": "workspace:*"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@types/node": "^18.15.13",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
