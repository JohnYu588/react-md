{
  "name": "docs-generator",
  "private": true,
  "version": "0.0.1-next.3",
  "description": "MDX Plugins for the documentation site",
  "type": "module",
  "exports": {
    "./*": "./dist/*.js"
  },
  "scripts": {
    "dev": "pnpm run watch",
    "watch": "npm-run-all --parallel build-esm-watch build-types-watch",
    "build": "npm-run-all build-esm build-types",
    "build-esm": "swc -d ./dist --strip-leading-paths src",
    "build-esm-watch": "pnpm run build-esm --watch",
    "build-types": "tsc -P tsconfig.types.json",
    "build-types-watch": "pnpm run build-types --watch",
    "prism-themes": "tsx src/scripts/create-prism-themes.ts",
    "clean-dist": "rm -rf dist",
    "clean": "rm -rf .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest"
  },
  "engines": {
    "pnpm": ">=9",
    "node": ">=20"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "@prettier/sync": "^0.5.2",
    "@react-md/code": "workspace:*",
    "@react-md/core": "workspace:*",
    "@types/estree-jsx": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/unist": "^3.0.2",
    "glob": "^10.4.2",
    "hast-util-heading-rank": "^3.0.0",
    "hast-util-properties-to-mdx-jsx-attributes": "^1.0.0",
    "mdast-util-from-markdown": "^2.0.1",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-mdx-jsx": "^3.1.2",
    "mdast-util-to-string": "^4.0.0",
    "micromark-extension-mdxjs": "^3.0.0",
    "postcss": "^8.4.39",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-remove-prefixes": "^1.2.0",
    "postcss-selector-parser": "^6.1.0",
    "postcss-sorting": "^8.0.2",
    "prettier": "^3.3.2",
    "rehype": "^13.0.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "sass": "^1.77.6",
    "sucrase": "^3.35.0",
    "ts-morph": "^23.0.0",
    "unified": "^11.0.5",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "vfile": "^6.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@mdx-js/mdx": "^3.0.1",
    "@mlaursen/eslint-config": "^4.1.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.6",
    "@swc/jest": "^0.2.29",
    "@types/node": "^20.14.10",
    "@types/postcss-sorting": "^8.0.2",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.4",
    "cssnano-preset-default": "^7.0.4",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "pretty-ms": "^9.0.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "volta": {
    "node": "20.12.2",
    "pnpm": "9.5.0"
  }
}
