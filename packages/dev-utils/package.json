{
  "name": "@react-md/dev-utils",
  "version": "5.1.0",
  "description": "Common build utilities for react-md. Contains helpers for building, watching, testing, etc",
  "private": true,
  "bin": {
    "dev-utils": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rimraf bin dist",
    "executable": "node executable.js",
    "compile": "tsc",
    "build": "npm-run-all compile executable",
    "watcher": "chokidar \"bin/cli.js\" -c \"npm run executable\"",
    "watch": "npm-run-all -p \"compile -w\" watcher"
  },
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@octokit/core": "^3.6.0",
    "chokidar": "^3.5.3",
    "commander": "^9.1.0",
    "cssnano": "^5.1.4",
    "dotenv": "^16.0.0",
    "filesize": "^8.0.7",
    "fs-extra": "^10.0.1",
    "glob": "^7.2.0",
    "gzip-size": "^6.0.0",
    "he": "^1.2.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.0",
    "markdown-toc": "^1.2.0",
    "marked": "^4.0.12",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.2",
    "prettier": "^2.6.0",
    "pretty-ms": "^7.0.0",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sassdoc": "^2.7.2",
    "ts-morph": "^14.0.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/he": "^1.1.2",
    "@types/marked": "^4.0.2",
    "@types/prompts": "^2.0.14",
    "@types/sass": "^1.43.1",
    "chokidar-cli": "^3.0.0"
  }
}
