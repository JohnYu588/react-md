{
  "name": "@react-md/dev-utils",
  "version": "2.5.0",
  "description": "Common build utilities for react-md. Contains helpers for building, watching, testing, etc",
  "private": true,
  "bin": {
    "dev-utils": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rimraf bin dist",
    "executable": "node executable.js",
    "compile": "tsc",
    "build": "npm-run-all compile executable",
    "watcher": "chokidar \"bin/cli.js\" -c \"npm run executable\"",
    "watch": "npm-run-all -p \"compile -w\" watcher"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "chokidar": "^3.5.0",
    "commander": "^6.0.0",
    "cpx": "^1.5.0",
    "cssnano": "^4.1.10",
    "filesize": "^6.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gzip-size": "^6.0.0",
    "he": "^1.2.0",
    "lodash": "^4.17.19",
    "loglevel": "^1.7.1",
    "markdown-toc": "^1.2.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "pretty-ms": "^7.0.0",
    "prompts": "^2.4.0",
    "rimraf": "^3.0.2",
    "sassdoc": "^2.7.2",
    "ts-morph": "^9.1.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/cssnano": "^4.0.0",
    "@types/he": "^1.1.1",
    "@types/prompts": "^2.0.8",
    "chokidar-cli": "^2.1.0"
  }
}
