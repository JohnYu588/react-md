{
  "name": "@react-md/codemod",
  "version": "6.0.1-next.0",
  "description": "ReactMD codemod scripts",
  "type": "module",
  "bin": {
    "rmd-codemod": "./dist/index.js"
  },
  "exports": {
    "./transforms/*": "./dist/transforms/*.js",
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "scripts": {
    "prepare-release": "npm-run-all clean-dist build",
    "clean-dist": "rm -rf dist",
    "clean": "rm -rf .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest",
    "build": "swc -d ./dist --strip-leading-paths src",
    "build-watch": "pnpm run build --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlaursen/react-md.git",
    "directory": "packages/codemod"
  },
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "keywords": [
    "react-md",
    "material design",
    "react",
    "codemod",
    "jscodeshift",
    "transforms"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@inquirer/confirm": "^5.1.10",
    "@inquirer/input": "^4.1.10",
    "@inquirer/rawlist": "^4.1.1",
    "@inquirer/search": "^3.0.13",
    "commander": "^13.1.0",
    "glob": "^11.0.2",
    "jscodeshift": "^17.3.0",
    "postcss": "^8.5.3",
    "postcss-scss": "^4.0.9",
    "postcss-value-parser": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@mlaursen/eslint-config": "^8.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.22",
    "@swc/jest": "^0.2.38",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/glob": "^8.1.0",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^22.15.18",
    "eslint": "^9.26.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "22.13.1",
    "pnpm": "10.8.0"
  }
}
