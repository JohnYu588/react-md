{
  "name": "@react-md/codemod",
  "type": "module",
  "version": "6.0.0-next.4",
  "description": "ReactMD codemod scripts",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "bin": {
    "rmd-codemod": "./bin/rmd-codemod.js"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "run-script": "tsx --tsconfig scripts/tsconfig.json",
    "update-docs": "node updateDocs.cjs",
    "prepare-release": "npm-run-all clean-dist build",
    "clean-dist": "rm -rf bin",
    "clean": "rm -rf .turbo bin node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "test": "jest",
    "build": "tsc --project tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlaursen/react-md.git",
    "directory": "packages/codemod"
  },
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "keywords": [
    "react-md",
    "material design",
    "react",
    "codemod",
    "jscodeshift",
    "transforms"
  ],
  "license": "MIT",
  "dependencies": {
    "commander": "^13.1.0",
    "glob": "^11.0.1",
    "jscodeshift": "^17.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@mlaursen/eslint-config": "^7.1.0",
    "@swc/cli": "^0.7.2",
    "@swc/core": "^1.11.21",
    "@swc/jest": "^0.2.37",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/glob": "^8.1.0",
    "@types/jscodeshift": "^0.12.0",
    "@types/node": "^22.14.1",
    "eslint": "^9.24.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "22.13.1",
    "pnpm": "10.8.0"
  }
}
