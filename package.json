{
  "name": "react-md-monorepo",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "description": "The monorepo for react-md",
  "scripts": {
    "prepare": "husky install",
    "cli": "ts-node -P scripts/tsconfig.json --swc",
    "test": "jest",
    "create-docs": "typedoc",
    "build-ejs": "swc src -d ejs",
    "lint-scripts": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint-styles": "stylelint \"**/*.{css,scss}\"",
    "lint": "pnpm run lint-scripts && pnpm run lint-styles",
    "typecheck-main": "tsc --noEmit",
    "typecheck-scripts": "tsc --noEmit --project scripts/tsconfig.json",
    "typecheck": "pnpm run typecheck-main && pnpm run typecheck-scripts",
    "format": "prettier --write .",
    "prismjs-themes": "pnpm run cli scripts/prismThemes.ts",
    "update-material-icons": "pnpm run cli scripts/updateMaterialIcons.ts",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-static": "next build && next export"
  },
  "engines": {
    "pnpm": ">=7",
    "node": ">=16"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "cnbuilder": "^3.1.0",
    "filesize": "^10.0.5",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "^13.0.4",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-marked-renderer": "^1.1.2",
    "react-window": "^1.8.8"
  },
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@mlaursen/eslint-config": "^1.9.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.18",
    "@swc/jest": "^0.2.23",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/glob": "^8.0.0",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.2.3",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.9",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-window": "^1.8.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "browserslist": "^4.21.4",
    "commander": "^9.4.1",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.4",
    "eslint-plugin-next": "^0.0.0",
    "glob": "^8.0.3",
    "husky": "^8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^9.1.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "prism-themes": "^1.9.0",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^5.0.0",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.14",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "**/*.{scss,js,jsx,md,yml,json,ts,tsx}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}
