{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "@react-md/codemod#build": {
      "outputs": ["bin/**"]
    },
    "@react-md/core#build": {
      "outputs": ["dist/**"]
    },
    "@react-md/material-icons#build": {
      "outputs": ["*.d.ts", "*.js"]
    },
    "docs#build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "typecheck": {}
  }
}
