{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "@react-md/material-icons#build": {
      "cache": false,
      "outputs": ["*.d.ts", "*.js"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "types/**", ".next/**", "!.next/cache/**"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "typecheck": {}
  }
}
