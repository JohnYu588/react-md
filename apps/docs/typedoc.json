{
  "$schema": "https://typedoc.org/schema.json",
  "out": "public/typedoc",
  "tsconfig": "../../packages/core/tsconfig.types.json",
  "highlightLanguages": [
    "bash",
    "console",
    "css",
    "scss",
    "diff",
    "html",
    "javascript",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "tsx",
    "typescript"
  ],
  "exclude": ["**/__tests__/**"],
  "readme": "../../packages/core/README.md",
  "entryPoints": ["../../packages/core/src"],
  "entryPointStrategy": "expand"
}
