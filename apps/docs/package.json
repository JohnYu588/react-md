{
  "name": "docs",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "description": "The documentation site for react-md",
  "scripts": {
    "create-env": "pnpm run run-script scripts/createEnv.ts",
    "mdx-pages": "pnpm run run-script scripts/createMarkdownPages.ts",
    "mdx-pages-watch": "pnpm run mdx-pages -- --watch",
    "mdx-pages-clean": "pnpm run mdx-pages -- --clean-only",
    "next-dev": "next dev",
    "next-build": "next build",
    "watchers": "npm-run-all -p mdx-pages-watch next-dev",
    "dev": "npm-run-all create-env watchers",
    "build": "npm-run-all create-env mdx-pages next-build",
    "start": "next start",
    "clean-static": "rm -rf .turbo .next node_modules",
    "clean": "npm-run-all clean-static mdx-pages-clean",
    "typecheck-src": "tsc --noEmit",
    "typecheck-scripts": "tsc --noEmit -P scripts/tsconfig.json",
    "typecheck": "npm-run-all typecheck-src typecheck-scripts",
    "lint-scripts": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint-styles": "stylelint \"src/**/*.{css,scss}\"",
    "lint": "npm-run-all lint-scripts lint-styles",
    "run-script": "ts-node -P scripts/tsconfig.json",
    "prism-themes": "pnpm run run-script scripts/prismThemes.ts"
  },
  "engines": {
    "pnpm": ">=8",
    "node": "^18"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@next/mdx": "^13.5.3",
    "@react-md/core": "workspace:*",
    "@react-md/material-icons": "workspace:*",
    "cnbuilder": "^3.1.0",
    "github-slugger": "^2.0.0",
    "js-cookie": "^3.0.5",
    "next": "^13.5.3",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1",
    "sucrase": "^3.34.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^2.3.0",
    "@mlaursen/eslint-config": "^3.0.0",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.199",
    "@types/mdx": "^2.0.8",
    "@types/node": "^20.8.0",
    "@types/prismjs": "^1.26.1",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "chokidar": "^3.5.3",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.3",
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "pretty-ms": "^8.0.0",
    "prism-themes": "^1.9.0",
    "sass": "^1.68.0",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-order": "^6.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
