{
  "name": "docs",
  "type": "module",
  "private": true,
  "version": "0.0.11",
  "description": "The documentation site for react-md",
  "scripts": {
    "run-script": "tsx --tsconfig scripts/tsconfig.json",
    "create-env": "pnpm run run-script scripts/createEnv.ts",
    "create-scss-lookup": "pnpm run run-script scripts/createScssLookup.ts",
    "create-stackblitz-template": "pnpm run run-script scripts/createStackBlitzTemplate.ts",
    "update-algolia-index": "pnpm run run-script scripts/updateAlgoliaIndex.ts",
    "typecheck-src": "tsc --noEmit",
    "typecheck-scripts": "tsc --noEmit -P scripts/tsconfig.json",
    "typecheck": "npm-run-all typecheck-src typecheck-scripts",
    "lint-scripts": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint-styles": "stylelint \"src/**/*.{css,scss}\"",
    "lint": "npm-run-all lint-scripts lint-styles",
    "clean-static": "rm -rf .turbo .next node_modules",
    "clean-generated": "rm -rf src/generated public/typedoc",
    "clean": "npm-run-all clean-static clean-generated",
    "sassdoc": "pnpm run run-script scripts/sassdoc.ts",
    "watch-demos": "pnpm run run-script scripts/watchDemos.ts",
    "watch-scss-lookup": "pnpm create-scss-lookup -- --watch",
    "generator-dev": "nodemon --config scripts/nodemon.docs-generator.json",
    "build-env": "npm-run-all --parallel create-env create-scss-lookup create-stackblitz-template",
    "build-full-env": "npm-run-all --parallel build-env sassdoc build-typedoc",
    "build-typedoc": "pnpm run run-script scripts/typedoc.ts",
    "next-dev": "next dev",
    "next-build": "next build",
    "dev": "npm-run-all build-env \"create-env --dev\" dev-tasks",
    "dev-tasks": "npm-run-all --parallel watch-demos \"sassdoc --touch\" watch-scss-lookup generator-dev",
    "build": "npm-run-all build-full-env next-build",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/mdx": "^3.1.1",
    "@next/mdx": "^14.2.15",
    "@next/third-parties": "^14.2.15",
    "@react-md/code": "workspace:*",
    "@react-md/core": "workspace:*",
    "@react-md/material-icons": "workspace:*",
    "algoliasearch": "^5.39.0",
    "cnbuilder": "^3.1.0",
    "docs-generator": "workspace:*",
    "filesize": "^11.0.13",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "js-cookie": "^3.0.5",
    "next": "^14.2.15",
    "next-mdx-remote": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "^1.8.11",
    "server-only": "^0.0.1",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@algolia/client-search": "^5.39.0",
    "@eslint/eslintrc": "^3.3.1",
    "@inquirer/confirm": "^5.1.18",
    "@next/eslint-plugin-next": "^15.5.4",
    "@react-md/eslint-config": "workspace:*",
    "@react-md/stylelint-config": "workspace:*",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.20",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.15.29",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-window": "^1.8.8",
    "chokidar": "^4.0.3",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "glob": "^11.0.3",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "sassdoc-generator": "workspace:*",
    "stylelint": "^16.24.0",
    "svgo": "^4.0.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.13",
    "typescript": "^5.8.3"
  },
  "volta": {
    "node": "22.13.1",
    "pnpm": "10.8.0"
  }
}
