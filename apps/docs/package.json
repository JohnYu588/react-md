{
  "name": "docs",
  "type": "module",
  "private": true,
  "version": "6.0.0-next.0",
  "description": "The documentation site for react-md",
  "scripts": {
    "create-env": "pnpm run run-script scripts/createEnv.ts",
    "mdx-pages": "pnpm run run-script scripts/createMarkdownPages.ts",
    "mdx-pages-watch": "pnpm run mdx-pages -- --watch",
    "mdx-pages-clean": "pnpm run mdx-pages -- --clean-only",
    "scss-lookup": "pnpm run run-script scripts/createScssLookup.ts",
    "scss-lookup-clean": "pnpm run scss-lookup -- --clean-only",
    "scss-lookup-watch": "pnpm run scss-lookup -- --watch",
    "create-api-docs": "pnpm run run-script scripts/createApiDocs.ts",
    "next-dev": "next dev",
    "next-build": "next build",
    "watchers": "npm-run-all -p mdx-pages-watch scss-lookup-watch next-dev",
    "dev": "npm-run-all create-env create-api-docs watchers",
    "build": "npm-run-all create-env create-api-docs mdx-pages next-build",
    "start": "next start",
    "clean-static": "rm -rf .turbo .next node_modules",
    "clean": "npm-run-all mdx-pages-clean clean-static",
    "typecheck-src": "tsc --noEmit",
    "typecheck-scripts": "tsc --noEmit -P scripts/tsconfig.json",
    "typecheck": "npm-run-all typecheck-src typecheck-scripts",
    "lint-scripts": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint-styles": "stylelint \"src/**/*.{css,scss}\"",
    "lint": "npm-run-all lint-scripts lint-styles",
    "run-script": "ts-node -P scripts/tsconfig.json",
    "prism-themes": "pnpm run run-script scripts/prismThemes.ts"
  },
  "engines": {
    "pnpm": ">=8",
    "node": "^18"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@next/mdx": "^14.0.1",
    "@react-md/core": "workspace:*",
    "@react-md/material-icons": "workspace:*",
    "cnbuilder": "^3.1.0",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "js-cookie": "^3.0.5",
    "marked": "^9.1.5",
    "next": "^14.0.1",
    "postcss-selector-parser": "^6.0.13",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.9",
    "server-only": "^0.0.1",
    "sucrase": "^3.34.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.0",
    "@mlaursen/eslint-config": "^4.0.0",
    "@types/js-cookie": "^3.0.5",
    "@types/lodash": "^4.14.200",
    "@types/mdx": "^2.0.9",
    "@types/node": "^20.8.10",
    "@types/postcss-sorting": "^8.0.2",
    "@types/prismjs": "^1.26.2",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@types/react-window": "^1.8.7",
    "chokidar": "^3.5.3",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.1",
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-remove-prefixes": "^1.2.0",
    "postcss-sorting": "^8.0.2",
    "prettier": "^3.0.3",
    "pretty-ms": "^8.0.0",
    "prism-themes": "^1.9.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-order": "^6.0.3",
    "svgo": "^3.0.2",
    "ts-morph": "^20.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
