{
  "name": "docs",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "description": "The documentation site for react-md",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf .turbo .next node_modules",
    "typecheck": "tsc --noEmit",
    "lint-scripts": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint-styles": "stylelint \"src/**/*.{css,scss}\"",
    "lint": "npm-run-all lint-scripts lint-styles",
    "run-script": "ts-node -P scripts/tsconfig.json",
    "prism-themes": "pnpm run run-script scripts/prismThemes.ts"
  },
  "engines": {
    "pnpm": ">=8",
    "node": "^18"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
