  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@next/mdx": "^13.5.1",
    "@react-md/core": "workspace:*",
    "@react-md/material-icons": "workspace:*",
    "cnbuilder": "^3.1.0",
    "js-cookie": "^3.0.5",
    "next": "^13.5.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
