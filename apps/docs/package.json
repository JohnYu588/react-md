{
  "name": "docs",
  "type": "module",
  "private": true,
  "version": "0.0.2-next.3",
  "description": "The documentation site for react-md",
  "scripts": {
    "run-script": "tsx --tsconfig scripts/tsconfig.json",
    "create-env": "pnpm run run-script scripts/createEnv.ts",
    "create-scss-lookup": "pnpm run run-script scripts/createScssLookup.ts",
    "typecheck-src": "tsc --noEmit",
    "typecheck-scripts": "tsc --noEmit -P scripts/tsconfig.json",
    "typecheck": "npm-run-all typecheck-src typecheck-scripts",
    "lint-scripts": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint-styles": "stylelint \"src/**/*.{css,scss}\"",
    "lint": "npm-run-all lint-scripts lint-styles",
    "clean-static": "rm -rf .turbo .next node_modules",
    "clean-generated": "rm -rf src/generated",
    "clean": "npm-run-all clean-static clean-generated",
    "watch-demos": "pnpm run run-script scripts/watchDemos.ts",
    "watch-scss-lookup": "pnpm create-scss-lookup -- --watch",
    "generator-dev": "nodemon --config scripts/nodemon.docs-generator.json",
    "build-env": "npm-run-all create-env create-scss-lookup",
    "next-dev": "next dev",
    "next-build": "next build",
    "dev": "npm-run-all build-env dev-tasks",
    "dev-tasks": "npm-run-all --parallel watch-demos watch-scss-lookup next-dev",
    "build": "npm-run-all build-env next-build",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/mdx": "^3.1.0",
    "@next/mdx": "^14.2.15",
    "@next/third-parties": "^14.2.15",
    "@react-md/code": "workspace:*",
    "@react-md/core": "workspace:*",
    "@react-md/material-icons": "workspace:*",
    "autosuggest-highlight": "^3.3.4",
    "cnbuilder": "^3.1.0",
    "docs-generator": "workspace:*",
    "fuse.js": "^7.0.0",
    "js-cookie": "^3.0.5",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "^1.8.10",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^5.1.3",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.12",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.7.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-window": "^1.8.8",
    "chokidar": "^4.0.1",
    "eslint": "^9.13.0",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "sass": "^1.80.3",
    "stylelint": "^16.10.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.8.1",
    "svgo": "^3.3.2",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4"
  },
  "volta": {
    "node": "20.12.2",
    "pnpm": "9.12.1"
  }
}
