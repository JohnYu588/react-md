{
  "name": "docs-v2",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "description": "The documentation site for react-md",
  "scripts": {
    "run-script": "tsx --tsconfig scripts/tsconfig.json",
    "create-env": "pnpm run run-script scripts/createEnv.ts",
    "create-scss-lookup": "pnpm run run-script scripts/createScssLookup.ts",
    "typecheck-src": "tsc --noEmit",
    "typecheck-scripts": "tsc --noEmit -P scripts/tsconfig.json",
    "typecheck": "npm-run-all typecheck-src typecheck-scripts",
    "lint-scripts": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint-styles": "stylelint \"src/**/*.{css,scss}\"",
    "lint": "npm-run-all lint-scripts lint-styles",
    "clean": "rm -rf .turbo .next node_modules src/generated",
    "mdx-dev": "nodemon --config nodemon.docs-generator.json",
    "next-dev": "next dev",
    "next-build": "next build",
    "dev": "npm-run-all create-env next-dev",
    "build": "npm-run-all create-env create-scss-lookup next-build",
    "start": "next start"
  },
  "engines": {
    "pnpm": ">=8",
    "node": "^18"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/mdx": "^3.0.1",
    "@next/mdx": "^14.2.1",
    "@react-md/code": "workspace:*",
    "@react-md/core": "workspace:*",
    "@react-md/material-icons": "workspace:*",
    "cnbuilder": "^3.1.0",
    "docs-generator": "workspace:*",
    "fuse.js": "^7.0.0",
    "js-cookie": "^3.0.5",
    "next": "^14.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.10",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^4.1.1",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@types/react-window": "^1.8.8",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.1",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "stylelint": "^16.3.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.2.1",
    "svgo": "^3.2.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
