    "@types/lodash": "^4.14.198",
    "@types/mdx": "^2.0.7",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.5.1",
    "glob": "^10.3.4",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prism-themes": "^1.9.0",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier-scss": "^1.0.0",
